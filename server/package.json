    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "copyfiles": "^2.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.7",
    "typescript": "^5.6.3"
  }
}
